# Configurações da API
API_KEY=your_admin_master_api_key_here
BASE_WEBHOOK_URL=http://localhost:3000
MAX_ATTACHMENT_SIZE=10000000
SET_MESSAGES_AS_SEEN=false
DISABLED_CALLBACKS=
ENABLE_LOCAL_CALLBACK_EXAMPLE=false
ENABLE_SWAGGER_ENDPOINT=true
WEB_VERSION=2.2402.5
WEB_VERSION_CACHE_TYPE=none
RATE_LIMIT_MAX=1000
RATE_LIMIT_WINDOW_MS=1000
RECOVER_SESSIONS=true

# Configurações do PostgreSQL
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=whatsapp_auth
POSTGRES_USER=whatsapp_user
POSTGRES_PASSWORD=your_postgres_password_here

# Configurações JWT
JWT_SECRET=your_jwt_secret_here_change_this_in_production
JWT_EXPIRES_IN=24h
JWT_REFRESH_EXPIRES_IN=7d

# Configurações de Autenticação
ENABLE_AUTH=true

# Configurações do Redis (se necessário)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password_here 